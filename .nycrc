{
  "nyc": {
    "include": [
      "src/**/*.js",
      "src/**/*.ts",
      "src/**/*.jsx",
      "src/**/*.tsx",
      "workers/**/*.js",
      "workers/**/*.ts"
    ],
    "exclude": [
      "test/**",
      "tests/**",
      "node_modules/**",
      "dist/**",
      "dist-worker/**",
      "dist-worker-optimized/**"
    ],
    "extension": [
      ".js",
      ".ts",
      ".jsx",
      ".tsx"
    ],
    "reporter": [
      "text",
      "lcov",
      "html",
      "cobertura"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 95,
    "branches": 90,
    "functions": 100,
    "lines": 98
  }
}